            <BeanShellPostProcessor guiclass="TestBeanGUI" testclass="BeanShellPostProcessor" testname="BeanShell PostProcessor" enabled="true">
              <boolProp name="resetInterpreter">false</boolProp>
              <stringProp name="parameters"></stringProp>
              <stringProp name="filename"></stringProp>
              <stringProp name="script">import org.apache.commons.io.FileUtils;

log.info(&quot;Определение совпадения национальностей&quot;);

int length = Integer.parseInt(vars.get(&quot;country_id_matchNr&quot;));
String nat = vars.get(&quot;nationality&quot;);
String nameFirst = vars.get(&quot;nameFirst&quot;);
nameFirst = nameFirst.toLowerCase();
log.info(nameFirst);

log.info(&quot;Начало цикла. (длина массива: &quot; + length.toString() + &quot;)&quot;);

for(int i = 1; i &lt;= length; i++) {	
	String s = &quot;country_id_&quot; + i.toString();
	if (nat.equals(vars.get(s))) {
		vars.put(&quot;match&quot;, &quot;true&quot;);

		//сохранение фото
		String image = vars.get(&quot;large&quot;);
		log.info(image);
		URL url = new URL(image);
		InputStream input = url.openStream();
		String pathPhoto = &quot;C:\\JMETER\\photo\\&quot; + nameFirst + &quot;.png&quot;;
		vars.put(&quot;pathPhoto&quot;, pathPhoto);
		String pathCSV = &quot;C:\\JMETER\\&quot; + nameFirst + &quot;.csv&quot;;
		byte[] buffer = input.readAllBytes();
		log.info(pathPhoto);
		File savedImageFile = new File(pathPhoto);
		FileUtils.writeByteArrayToFile(savedImageFile, buffer);
		log.info(&quot;Изображение сохранено&quot;);

		//сохраняем файл csv
		File csvFile = new File(pathCSV);	
		FileOutputStream fileOutputStream = new FileOutputStream(csvFile);
		FileWriter fw = new FileWriter(csvFile);		
		fw.write(vars.get(&quot;nameFirst&quot;));
		fw.write(&quot;,&quot;);
		fw.write(vars.get(&quot;nameLast&quot;));
		fw.write(&quot;,&quot;);
		fw.write(vars.get(&quot;nationality&quot;));
		fw.write(&quot;,&quot;);
		fw.write(vars.get(&quot;pathPhoto&quot;));
		fw.close();

		break;
	}
}</stringProp>
